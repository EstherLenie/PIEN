    "version": "0.2.0",
    "configurations": [

        {
            "name": "Launch Utilisateur",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/services/go/utilisateur/cmd",
            "output": "${workspaceFolder}/services/go/utilisateur/utilisateur"
        },
        {
            "name": "Launch Cours",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/services/go/cours/cmd",
            "output": "${workspaceFolder}/services/go/cours/cours"

        },
        {
            "name": "Launch Multimedia",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/services/go/multimedia/cmd",
            "output": "${workspaceFolder}/services/go/multimedia/multimedia",
            "cwd": "${workspaceFolder}/services/go/multimedia"
        }
    ]
}